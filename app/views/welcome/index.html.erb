<h1>Welcome to Viewing Party!</h1>

<% if current_user %>
  <h3>Current Users:</h3>
  <hr>
  <% @users.each do |user| %>
    <h3><%= link_to "#{user.name}", "/dashboard" %></h3>
    <p><%= user.email %></p>
    <hr>
  <% end %>
  <%= link_to "Log Out", "/logout", method: :delete %>
<% else %>
  <%= link_to "Log In", "/login", method: :get %><br>
  <br><%= button_to "Create New User", "/register", method: :get %><br>
<% end %>
